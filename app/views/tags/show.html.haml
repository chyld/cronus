%div.tags
  %div.nav
    = link_to image_tag("arrow_left.png", :border => 0), tag_path(@tag.prev)
    = link_to image_tag("arrow_right.png", :border => 0), tag_path(@tag.next)
  %div.head= link_to @tag.name, edit_tag_path(@tag)
  - @tag.notes.each do |note|
    %div.item
      = link_to note.title, note_path(note)
